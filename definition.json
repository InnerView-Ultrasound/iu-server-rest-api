{
   "swagger": "2.0",
   "info": {
      "description": "This is the API spec for InnerView's AppointmentQuest wrapper. May be updated to use Square for appointments in the future. *(Based on Swagger Editor's default 'Pet Shop' example and subject to heavy change)* For this sample, you can use the api key `special-key` to test the authorization filters.",
      "version": "1.0",
      "title": "IU Server API (AppointmentQuest)",
      "termsOfService": "http://swagger.io/terms/",
      "contact": {
         "email": "psiuk@braiden.net"
      },
      # "license": {
      #   "name": "Apache 2.0",
      #   "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
      # }
   },
   "host": "innerview.host",
   "basePath": "/api/v1",
   "tags": [
      # {
      #   "name": "pet",
      #   "description": "Everything about your Pets",
      #   "externalDocs": {
      #       "description": "Find out more",
      #       "url": "http://swagger.io"
      #   }
      # },
      # {
      #   "name": "user",
      #   "description": "Operations about user",
      #   "externalDocs": {
      #       "description": "Find out more about our store",
      #       "url": "http://swagger.io"
      #   }
      # },
      {
         "name": "customer",
         "description": "Access to customer information",
         "externalDocs": {
            "description": "AppointmentQuest homepage",
            "url": "https://appointmentquest.com"
         }
      }
   ],
   "schemes": [
      # "https",
      "http"
   ],
   "paths": {
      "/basic-client-info/today": {
         "get": {
            "tags": [
               "customer"
            ],
            "summary": "Returns basic information for all clients on the schedule for the current day",
            "description": "Returns basic information for all clients on the schedule for today. Does not currentlty have an option for specifying which ultrasound tech (AQ Personnel) is currently seeing the client. Needs to return only ACTUAL appointments and not cancelled ones.",
            "operationId": "getBasicClientInfoForCurrentDay",
            "produces": [
               "application/json"
            ],
            "parameters": [],
            "responses": {
               "200": {
                  "description": "successful operation",
                  "schema": {
                     "type": "object",
                     "additionalProperties": {
                        "type": "integer",
                        "format": "int32"
                     }
                  }
               }
            },
            "security": [
               {
                  "api_key": []
               }
            ]
         }
      }
   },
   "securityDefinitions": {
      "petstore_auth": {
         "type": "oauth2",
         "authorizationUrl": "http://petstore.swagger.io/oauth/dialog",
         "flow": "implicit",
         "scopes": {
            "write:pets": "modify pets in your account",
            "read:pets": "read your pets"
         }
      },
      "api_key": {
         "type": "apiKey",
         "name": "api_key",
         "in": "header"
      }
   },
   "definitions": {
      "Order": {
         "type": "object",
         "properties": {
            "id": {
               "type": "integer",
               "format": "int64"
            },
            "petId": {
               "type": "integer",
               "format": "int64"
            },
            "quantity": {
               "type": "integer",
               "format": "int32"
            },
            "shipDate": {
               "type": "string",
               "format": "date-time"
            },
            "status": {
               "type": "string",
               "description": "Order Status",
               "enum": [
                  "placed",
                  "approved",
                  "delivered"
               ]
            },
            "complete": {
               "type": "boolean",
               "default": false
            }
         },
         "xml": {
            "name": "Order"
         }
      },
      "Category": {
         "type": "object",
         "properties": {
            "id": {
               "type": "integer",
               "format": "int64"
            },
            "name": {
               "type": "string"
            }
         },
         "xml": {
            "name": "Category"
         }
      },
      "User": {
         "type": "object",
         "properties": {
            "id": {
               "type": "integer",
               "format": "int64"
            },
            "username": {
               "type": "string"
            },
            "firstName": {
               "type": "string"
            },
            "lastName": {
               "type": "string"
            },
            "email": {
               "type": "string"
            },
            "password": {
               "type": "string"
            },
            "phone": {
               "type": "string"
            },
            "userStatus": {
               "type": "integer",
               "format": "int32",
               "description": "User Status"
            }
         },
         "xml": {
            "name": "User"
         }
      },
      "Tag": {
         "type": "object",
         "properties": {
            "id": {
               "type": "integer",
               "format": "int64"
            },
            "name": {
               "type": "string"
            }
         },
         "xml": {
            "name": "Tag"
         }
      },
      "Pet": {
         "type": "object",
         "required": [
            "name",
            "photoUrls"
         ],
         "properties": {
            "id": {
               "type": "integer",
               "format": "int64"
            },
            "category": {
               "$ref": "#/definitions/Category"
            },
            "name": {
               "type": "string",
               "example": "doggie"
            },
            "photoUrls": {
               "type": "array",
               "xml": {
                  "name": "photoUrl",
                  "wrapped": true
               },
               "items": {
                  "type": "string"
               }
            },
            "tags": {
               "type": "array",
               "xml": {
                  "name": "tag",
                  "wrapped": true
               },
               "items": {
                  "$ref": "#/definitions/Tag"
               }
            },
            "status": {
               "type": "string",
               "description": "pet status in the store",
               "enum": [
                  "available",
                  "pending",
                  "sold"
               ]
            }
         },
         "xml": {
            "name": "Pet"
         }
      },
      "ApiResponse": {
         "type": "object",
         "properties": {
            "code": {
               "type": "integer",
               "format": "int32"
            },
            "type": {
               "type": "string"
            },
            "message": {
               "type": "string"
            }
         }
      }
   },
   "externalDocs": {
      "description": "Visit InnerView Ultrasound's homepage",
      "url": "https://innerviewultrasound.com"
   }
}
